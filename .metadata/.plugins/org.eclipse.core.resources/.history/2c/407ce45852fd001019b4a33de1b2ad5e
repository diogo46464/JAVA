package entities;

public class Student {
		
		public String name;
		public double nota1;
		public double nota2;
		public double nota3;
		public double faltam;
		public double total;
		
		
		public double notaFinal() {
			 return total =  nota1 + nota2+ nota3 / 3.0;
		}
		
		public double faltam() {
			
			return faltam = 60.00 - total;
		}

}	
