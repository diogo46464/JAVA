import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int n  =sc.nextInt();
		
		int valores = 0;
		
		for(int i = 0; i < n; i++) {
			
			 valores = sc.nextInt();
			
			valores++;
		
		}
		
		System.out.println(valores);
		
		sc.close();
		
		
	}

}
