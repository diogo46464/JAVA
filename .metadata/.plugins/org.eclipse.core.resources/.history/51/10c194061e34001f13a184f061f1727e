package controle;

import java.util.Scanner;

public class WhileIndeterminado {
	
	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		String digite = null;
		while(digite == "sair" ) {
			System.out.println("Digite algo: ");
			digite = entrada.next();
		}
		
		
		
		
		entrada.close();
		
		
		
	}

}
