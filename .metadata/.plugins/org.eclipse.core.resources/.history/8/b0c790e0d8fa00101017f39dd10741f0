import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner (System.in);
		
		
		int X = sc.nextInt();

		
		for(int i = 1; X <= 1000; i++) {
			
			int valores = sc.nextInt();
			
			
			if(valores % 2 != 0) {
				System.out.println(valores); 
			}
			
		}
		
		
		
		
		
		sc.close();
		
	}

}
